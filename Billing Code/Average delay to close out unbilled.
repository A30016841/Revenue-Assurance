SELECT INSTALLATION_STATE, METER_TYPE, ROUND(AVG(DAYS_BETWEEN(DAY_IN, DAY_OUT))) AS AVG_RESOLUTION_TIME
FROM "COMMERCIAL"."HNODS_ST_BILL_UNBILLED_TREND" 
WHERE DAY_OUT IS NOT NULL
AND (DAY_OUT <> ADD_DAYS(DAY_IN,1) AND MRU NOT LIKE 'FBRM%')
AND FLAG = 'Un-Billed'
GROUP BY INSTALLATION_STATE,METER_TYPE
